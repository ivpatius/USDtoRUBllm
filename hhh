# в файле prices.txt запсианы стоимости хат

filename = "prices.txt" # это название файла где лежат хаты

max_prices = [] # Создаем массив для того чтобы туда цены добавлять

with open(filename, "r", encoding="utf-8") as file: # открываем файл просто крч
    for line in file: # бежим по данным файла
        line = line.strip() # выстраиваем строку массива чтобы она адекватной была
        if line == "":
            continue  # пропускаем пустые строки

        parts = line.split("\t")          # разделяем по табуляции
        nums = list(map(int, parts))      # превращаем в числа 
        max_prices.append(max(nums))      # добавляем самую дорогую на этаже

print(max_prices)



